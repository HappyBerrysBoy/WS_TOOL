{
    "name": "Steem Tools",
    "description": "Steem Tools",
    "version": "1.1.15",
    "permissions": [
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [{
        "matches": ["https://steemit.com/*"],
        "run_at": "document_idle",
        "js": [
            // "wstools_vue/dist/js/app.965dc514.js",
            // "wstools_vue/dist/js/chunk-vendors.bebe1101.js",
            "thirdParty/vue2.6.min.js",
            "thirdParty/main.js",
            "content_script.js"
        ],
        "css": [
            // "wstools_vue/dist/css/app.f1f10065.css"
        ]
    }],
    "browser_action": {
        "default_title": "Steem Tools",
        "default_icon": "steemit.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}